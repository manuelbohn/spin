 map2(eq, [], []),
 map2(eq, [0, 1, 2], [0, 1, 2]),
 map2(plus, [0, 1, 2], [0, 1, 2]),
 map2(plus, repeat(50000, function() { return 1}), repeat(50000, function() { return 2}))[49999],
